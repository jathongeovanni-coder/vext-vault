{
  "cleanUrls": true,
  "github": {
    "silent": true
  },
  "buildCommand": "trunk build --release --public-url /",
  "installCommand": "rustup target add wasm32-unknown-unknown && cargo fetch",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)\\.wasm$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
